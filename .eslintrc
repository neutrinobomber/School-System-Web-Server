{
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "eol-last": 0,
        "linebreak-style": 0,
        "quotes": [
            "error",
            "single"
        ],
        "require-jsdoc": 0
    }
}