{
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "extends": ["airbnb-base"],
  "rules": {
    "linebreak-style": 0,
    "consistent-return": 0,
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "no-restricted-syntax": 0,
    "no-else-return": 0
  }
}
